{
  "name": "react-wrappers",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build-wc && npm run build-dm && npm run build-grids && tsc && vite build",
    "preview": "vite preview",
    "build-wc": "tsx ./scripts/build-wc.ts",
    "build-dm": "tsx ./scripts/build-dm.ts",
    "build-grids": "tsx ./scripts/build-grids.ts",
    "test:browser": "vitest",
    "test:ci": "vitest --run --coverage",
    "lint": "biome lint --verbose",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@lit/react": "^1.0.7",
    "igniteui-dockmanager": "~1.16.1",
    "igniteui-webcomponents": "~6.1.1",
    "igniteui-webcomponents-grids": "~6.0.4",
    "lit": "^3.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.0.5",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "lefthook": "^1.11.14",
    "playwright": "^1.53.1",
    "prettier": "^3.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.0"
  }
}

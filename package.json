{
  "name": "react-wrappers",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build-wc && npm run build-dm && npm run build-grids && tsc && vite build",
    "preview": "vite preview",
    "build-wc": "tsx ./scripts/build-wc.ts",
    "build-dm": "tsx ./scripts/build-dm.ts",
    "build-grids": "tsx ./scripts/build-grids.ts",
    "test:browser": "vitest",
    "test:ci": "vitest --run --coverage",
    "lint": "biome lint --verbose",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@lit/react": "^1.0.7",
    "igniteui-dockmanager": "~1.16.1",
    "igniteui-webcomponents": "~6.0.0",
    "igniteui-webcomponents-grids": "~6.0.1",
    "lit": "^3.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "lefthook": "^1.11.11",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2",
    "vitest-browser-react": "^0.1.1"
  }
}
